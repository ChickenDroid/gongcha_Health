<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="gongcha_health.com">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Design">
        <activity android:name=".lottie_Activity"
            android:theme="@style/Theme.Design.NoActionBar"
            ></activity>
        <activity android:name=".Detail_Absanimation" />
        <activity android:name=".Detail_Absvideo" />
        <activity android:name=".after_training_abs" />
        <activity
            android:name=".training_abs"
            android:parentActivityName=".MainActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Detail_batsalanimation"
            android:parentActivityName=".after_traning_Batsal" />
        <activity
            android:name=".Detail_batsalvideo"
            android:parentActivityName=".after_traning_Batsal" />
        <activity
            android:name=".after_traning_Batsal"
            android:theme="@style/Theme.Design.NoActionBar" />
        <activity
            android:name=".Training_batsal"
            android:parentActivityName=".MainActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".MainActivity"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.Design.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".running_beginner"/>
        <activity android:name=".running_timer"/>
        <activity
            android:name=".LoadingActivity"
            android:theme="@style/Theme.Design.NoActionBar" />
    </application>

</manifest>