<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:https="http://schemas.android.com/apk/distribution"
    package="gongcha_health.com">
    <uses-permission android:name="android.permission.INTERNET" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher">

    <activity android:name=".Detail_Absanimation"></activity>
    <activity android:name=".Detail_Absvideo" />
    <activity android:name=".after_training_abs" />
    <activity
        android:name=".training_abs"
