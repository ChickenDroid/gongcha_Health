        android:screenOrientation="portrait" />
    <activity
        android:name=".Detail_batsalanimation"
        android:parentActivityName=".after_traning_Batsal" />
    <activity
        android:name=".Detail_batsalvideo"
        android:parentActivityName=".after_traning_Batsal" />
    <activity
